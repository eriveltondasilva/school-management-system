{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build"
  },
  "license": "MIT",
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "jsxSingleQuote": true,
    "printWidth": 100,
    "semi": false,
    "singleAttributePerLine": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "overrides": [
      {
        "files": ".prettierrc",
        "options": {
          "parser": "json"
        }
      }
    ],
    "tailwindFunctions": [
      "tw",
      "twJoin",
      "twMerge"
    ],
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "devDependencies": {
    "@headlessui/react": "^1.4.2",
    "@inertiajs/react": "^1.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.12",
    "axios": "^1.6.1",
    "laravel-vite-plugin": "^0.8.0",
    "postcss": "^8.4.18",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.1",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "flowbite": "^2.2.1",
    "flowbite-react": "^0.7.2",
    "lucide-react": "^0.298.0",
    "sass": "^1.69.5",
    "tailwind-merge": "^2.1.0"
  }
}
